----revinew raw data---

--for slass---
select sum(s.noofenrolledstudent * c.credithour)
FROM seas_section_t as s , seas_course_t as c
where s.course_id = c.courseID and c.dept_id= 'SLASS' and s.semester= 'Spring' and s.year = 2009

UNION ALL
select sum(s.noofenrolledstudent * c.credithour)
FROM seas_section_t as s , seas_course_t as c
where s.course_id = c.courseID and c.dept_id= 'SLASS' and s.semester= 'Summer' and s.year = 2009
UNION all 
select sum(s.noofenrolledstudent * c.credithour)
FROM seas_section_t as s , seas_course_t as c
where s.course_id = c.courseID and c.dept_id= 'SLASS' and s.semester= 'Autumn' and s.year = 2009

UNION ALL
select sum(s.noofenrolledstudent * c.credithour)
FROM seas_section_t as s , seas_course_t as c
where s.course_id = c.courseID and c.dept_id= 'SLASS' and s.semester= 'Spring' and s.year = 2010
UNION ALL
